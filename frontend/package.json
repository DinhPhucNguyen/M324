{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend-Projekt",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "webpack --mode production",
    "lint": "eslint .",
    "lint:css": "stylelint '**/*.css'",
    "test": "npx jest --passWithNoTests"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "jest": "^29.5.0",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": ["js", "jsx"],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js"]
  }
}
